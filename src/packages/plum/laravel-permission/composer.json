            "Plum\\LaravelPermission\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Plum\\LaravelPermission\\LaravelPermissionServiceProvider"
            ],
            "aliases": {
                "LaravelPermission": "Plum\\LaravelPermission\\Facades\\LaravelPermission"
            }
        }
    }
